			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_state_icon" type="list" default="" label="COM_SEMINARMAN_STATE_ICON">
			<option value="">COM_SEMINARMAN_USE_GLOBAL</option>		
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_sale" type="list" default="0" label="COM_SEMINARMAN_SALE_ICON">
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="language" type="languages" client="site" label="COM_SEMINARMAN_CONTENT_LANGUAGE" />
	</fieldset>
    </fields>
    <fields name="meta">
	<fieldset name="metadata">
		<field name="description" type="textarea" rows="5" cols="30" default="" label="COM_SEMINARMAN_DESCRIPTION" />
		<field name="keywords" type="textarea" rows="5" cols="30" default="" label="COM_SEMINARMAN_KEYWORDS" />
		<field name="robots" type="text" size="20" default="" label="COM_SEMINARMAN_ROBOTS" />
		<field name="author" type="text" size="20" default="" label="COM_SEMINARMAN_AUTHOR" />
	</fieldset>
    </fields>
</form>