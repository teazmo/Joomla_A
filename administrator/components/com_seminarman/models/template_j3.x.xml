<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="details">
		<field name="created_by" type="hidden" default="" label="COM_SEMINARMAN_CREATED_BY" />
		<field name="created_by_alias" type="text" size="20" default="" label="COM_SEMINARMAN_CREATED_BY_ALIAS" />
		<field name="publish_up" type="calendar" default="" label="COM_SEMINARMAN_PUBLISH_UP_DATE" format="%d.%m.%Y"/> <!-- Attribute "format" bitte auf den Wert von COM_SEMINARMAN_DATE_FORMAT1_ALT einstellen. Ist nicht Ã¼bersetzbar laut http://docs.joomla.org/Calendar_parameter_type -->
		<field name="publish_down" type="calendar" default="" label="COM_SEMINARMAN_PUBLISH_DOWN_DATE" format="%d.%m.%Y"/>
	</fieldset>
	<fields name="params">
	<fieldset name="basic">
			<field name="target" type="list" default="" label="COM_SEMINARMAN_TARGET" description="COM_SEMINARMAN_TARGET_DESC">
			<option value="">COM_SEMINARMAN_USE_GLOBAL</option>
			<option value="0">COM_SEMINARMAN_PARENT_WINDOW</option>
			<option value="1">COM_SEMINARMAN_NEW_WINDOW</option>
			<option value="2">COM_SEMINARMAN_NEW_WINDOW_WO</option>
		</field>
		<field name="show_tags" type="list" default="" label="COM_SEMINARMAN_SHOW_TAGS" description="COM_SEMINARMAN_SHOW_TAGS_DESC">
					<option value="">COM_SEMINARMAN_USE_GLOBAL</option>
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_favourites" type="list" default="" label="COM_SEMINARMAN_SHOW_FAVOURITES" description="COM_SEMINARMAN_SHOW_FAVOURITES_DESC">
				<option value="">COM_SEMINARMAN_USE_GLOBAL</option>
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_categories" type="list" default="" label="COM_SEMINARMAN_SHOW_CATEGORIES" description="COM_SEMINARMAN_SHOW_CATEGORIES_DESC">
				<option value="">COM_SEMINARMAN_USE_GLOBAL</option>
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_hyperlink" type="list" default="" label="COM_SEMINARMAN_SHOW_HYPERLINK" description="COM_SEMINARMAN_SHOW_HYPERLINK_DESC">
			<option value="">COM_SEMINARMAN_USE_GLOBAL</option>
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_experience_level" type="list" default="" label="COM_SEMINARMAN_SHOW_EXPERIENCE_LEVEL" description="COM_SEMINARMAN_SHOW_EXPERIENCE_LEVEL_DESC">
				<option value="">COM_SEMINARMAN_USE_GLOBAL</option>
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_location" type="list" default="" label="COM_SEMINARMAN_SHOW_LOCATION" description="COM_SEMINARMAN_SHOW_LOCATION_DESC">
			<option value="">COM_SEMINARMAN_USE_GLOBAL</option>
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_group" type="list" default="" label="COM_SEMINARMAN_SHOW_GROUP" description="COM_SEMINARMAN_SHOW_GROUP_DESC">
			<option value="">COM_SEMINARMAN_USE_GLOBAL</option>
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_pdf_icon" type="list" default="" label="COM_SEMINARMAN_PDF_ICON">
			<option value="">COM_SEMINARMAN_USE_GLOBAL</option>		
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
		<field name="show_print_icon" type="list" default="" label="COM_SEMINARMAN_PRINT_ICON">
			<option value="">COM_SEMINARMAN_USE_GLOBAL</option>		
			<option value="0">COM_SEMINARMAN_HIDE</option>
			<option value="1">COM_SEMINARMAN_SHOW</option>
		</field>
